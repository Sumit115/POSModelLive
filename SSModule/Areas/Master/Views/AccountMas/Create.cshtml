                                                        {
                                                            <tr index="@i">
                                                                <td class="p-2">
                                                                    @Html.TextBoxFor(m => m.AccountDtl_lst[i].Location, new { @readonly = true, @tabindex = "-1" })
                                                                    @Html.HiddenFor(m => m.AccountDtl_lst[i].FKLocationID)
                                                                    @Html.HiddenFor(m => m.AccountDtl_lst[i].PKAccountDtlId)

                                                                </td>

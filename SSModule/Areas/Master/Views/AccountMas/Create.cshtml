                        <div class="row" id="tab1" role="tabpanel" aria-labelledby="tab1-tab">


                            <div class="card card-detail">
                                @*<div class="card-header">
                                Locations
                                </div>*@
                                <div class="card-body">
                                    <div class="col-sm-12 mt-3">
                                        <div class="row form-group">
